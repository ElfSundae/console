{
    "name": "elfsundae/console",
    "type": "library",
    "description": "CLI library based on Laravel Console for creating PHP console application.",
    "keywords": ["console", "command", "cli", "laravel", "artisan", "script"],
    "homepage": "https://github.com/ElfSundae/console",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/console": "^5.4",
        "illuminate/container": "^5.4",
        "illuminate/events": "^5.4"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\Console\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "vlucas/phpdotenv": "Loads environment variables from .env to getenv(), $_ENV and $_SERVER automagically."
    }
}
