#!/usr/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';

try {
    (new Dotenv\Dotenv(__DIR__))->load();
} catch (Exception $e) {

}

(new ElfSundae\Console\Application)
    ->command('env', function () {
        $this->comment(env('ACCOUNT', 'default value'));
    })
    ->getApplication()
    ->runAsSingle();
